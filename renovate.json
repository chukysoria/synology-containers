{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>chukysoria/renovate-config",
    ":skipStatusChecks"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackagePrefixes": [
        "lscr.io/linuxserver"
      ],
      "versioning": "regex:^v?(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)(?:\\.|-ls)(?<build>\\d+)(?:-ls(?<revision>\\d+))?$"
    }
  ]
}